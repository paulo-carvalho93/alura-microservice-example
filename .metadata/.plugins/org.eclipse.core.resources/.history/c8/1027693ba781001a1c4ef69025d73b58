spring.application.name=zuul
eureka.instance.hostname=localhost
server.port=9090
 
zuul.prefix=/api
zuul.ignored-services=*
zuul.routes.restaurantes.path=/restaurantes/**
zuul.routes.restaurantes.serviceId=restaurantes
zuul.routes.restaurantes.strip-prefix=false

zuul.routes.tiposdecomida.path=/tiposdecomida/**
zuul.routes.tiposdecomida.serviceId=tiposdecomida
zuul.routes.tiposdecomida.strip-prefix=false

zuul.routes.pedidos.path=/pedidos/**
zuul.routes.pedidos.serviceId=pedidos
zuul.routes.pedidos.strip-prefix=false